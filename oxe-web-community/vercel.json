{
  "version": 2,
  "name": "openxeco-web",
  "buildCommand": "npm install && npm run build",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/build/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico))",
      "dest": "/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}